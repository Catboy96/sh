_goto() {
      local cur prev nodes
      COMPREPLY=()
      cur="${COMP_WORDS[COMP_CWORD]}"
      prev="${COMP_WORDS[COMP_CWORD-1]}"
      screens=$(ls /var/run/screen/S-$USER | grep -oP "(?<=\.).*")

      case $prev in
          goto)
              COMPREPLY=( $(compgen -W "${screens}" -- ${cur}) )
              return;;
      esac
} complete -o default -F _goto goto
